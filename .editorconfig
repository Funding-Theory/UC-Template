# editorconfig.org
root = true

[*]
indent_size = 4
charset = utf-8
end_of_line = lf
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

[*.yaml]
indent_style = space
trim_trailing_whitespace = true


[*.yml]
indent_style = space
trim_trailing_whitespace = true
